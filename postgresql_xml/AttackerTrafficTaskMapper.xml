<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/dtd">
<mapper namespace="com.dbapp.extension.xdr.threatMonitor.mapper.AttackerTrafficTaskMapper">

    <insert id="saveOrIgnoreBatch">
        insert ignore into t_attacker_traffic_task
        (ip, date_part, start_time, time_offset, history_time_offset, is_init)
        VALUES
        <foreach collection="tasks" item="task" separator=",">
            (#{task.ip}, #{task.datePart}, CAST(#{task.startTime} AS timestamp), #{task.timeOffset},
            #{task.historyTimeOffset}, #{task.isInit})
        </foreach>
    </insert>
</mapper>
