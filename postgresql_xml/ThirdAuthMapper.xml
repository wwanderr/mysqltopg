<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/dtd">
<mapper namespace="com.dbapp.extension.xdr.threatAnalysis.mapper.ThirdAuthMapper">

    <select id="getThirdAuth" resultType="java.lang.String">
        SELECT auth_tick
        FROM t_third_auth
        <where>
            <choose>
                <when test="machineCode != null and machineCode != ''">
                    machine_code = #{machineCode}
                </when>
                <otherwise>
                    #{ip} = ANY(STRING_TO_ARRAY(ip, ','))
                </otherwise>
            </choose>
        </where>
        ORDER BY update_time DESC
        LIMIT 1
    </select>

</mapper>
